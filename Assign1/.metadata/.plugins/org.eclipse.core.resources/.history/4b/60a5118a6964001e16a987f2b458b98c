
public class Star {

	public static void main(String[] args)
	{
		int num=5;
		int count=1;
		for(int i=0;i<num;i++)
         {
			for(int j=0;j<num-1;j++)
	         {
				System.out.println(" ");
				for(int k=0;k<count;k++)
				{
					System.out.print("*");
				}
				count++;
				System.out.println("");
	         }
         }
	 }
    }
