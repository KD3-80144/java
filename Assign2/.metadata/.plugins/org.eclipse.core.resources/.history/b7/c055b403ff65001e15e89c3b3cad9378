package com.app.geometry;

public class Point2D {
	private int x;
	private int y;
	
	public Point2D()
	{
		this(10,20);
	}
	public Point2D(int x, int y) 
	{
		super();
		this.x = x;
		this.y = y;
	}
	public boolean isEqualTo()
	{
		if(this.x==this.y;)
		    return true;
		else
			reurn false;
	}
	

}
